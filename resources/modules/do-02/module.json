{
  "module": "do-02",
  "title": "Датчик осадков ДО-02",
  "manufacture": "Minimax",
  "min_length": "20",
  "engine": "python3",
  "devices":
  {
    "precipitation":
    {
      "title": "Комплексный датчик погоды",
      "sensors": {
        "precipitation_type": {
          "title":"Тип осадков",
          "short_title":"Тип осадков",
          "unit":"",
          "mask":"@41:0\\s+\\S+\\s+\\S+\\s+\\S+\\s+\\S+\\s+\\S+\\s+(-*\\d+)",
          "rule":"($s_value == -9999)?$value = null:$value = $s_value;"
        },

        "precipitation_amount": {
          "title":"Количествово осадков",
          "short_title":"Кол-во осадков",
          "unit":"мм",
          "mask":"@41:0\\s+(-*\\d+)",
          "rule":"($s_value == -9999)?$value = null:$value = $s_value;"
        },
        "precipitation_intensity": {
          "title":"Интенсивность осадков",
          "short_title":"Инт. осадков",
          "unit":"мм/ч",
          "mask":"@41:0\\s+\\S+\\s+(-*\\d+)",
          "rule":"($s_value == -9999)?$value = null:$value = $s_value;"
        }
      }
    }
  }
}
